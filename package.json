{
  "name": "node-scalingo",
  "version": "0.1.2",
  "description": "Node client for Scalingo API",
  "keywords": [
    "Scalingo",
    "Node"
  ],
  "author": "Jérémy Buget",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/jbuget/node-scalingo/issues"
  },
  "homepage": "https://github.com/jbuget/node-scalingo#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbuget/node-scalingo.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/jest": "^25.1.1",
    "@types/node": "^13.7.0",
    "@types/sinon": "^7.5.1",
    "jest": "^25.1.0",
    "sinon": "^8.1.1",
    "ts-jest": "^25.1.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "moment": "^2.24.0"
  },
  "scripts": {
    "clean": "npm run clean:dependencies && npm run clean:dist",
    "clean:dist": "rm -rf dist",
    "clean:dependencies": "rm -rf node_modules",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run build && npm test",
    "release": "npm run release:minor",
    "release:major": "npm version major && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:patch": "npm version patch && npm publish"
  }
}
